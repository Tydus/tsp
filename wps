x   Profile                     G   /profile        {username,realname,role:Admin|Student|Professor}
x   Show Student                G   /student    
                                                    {student:[{
                                                        username,
                                                        realname,
                                                        cls,
                                                        cls_index,
                                                        selected:id,
                                                        applied_to:id,
