                                                    }]}
        (Filter)
x   Show Subject                G   /subject        {subject:[{
                                                        id,
                                                        name,
                                                        type1,
                                                        type2,
                                                        source,
                                                        selected_by:[{username,realname}],
                                                        applied_to:{username,realname},
                                                        professor:{realname,title,direction},
                                                    }]}
        (Filter)
